}

function DARIJA {
	header_setinterface="ختار ملتقت الشبكات لبغيتي تخدم بيه"
	setinterface_error="لا يوجد يوجد اي ملتقط ، إباي ..."

	# >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>
	header_choosescan="اختر قناة"
	choosescan_option_1="جميع القنوات "
	choosescan_option_2="قنوات محددة"
	# >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>
	scanchan_option_1="قناة واحدة"
	scanchan_option_2="قنوات متعددة"
