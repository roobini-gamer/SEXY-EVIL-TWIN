	header_scan="واي فاي مراقب"
	# >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>
	header_scanchan="مسح الهدف"
	# >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>
	header_askAP="حدد الخيار هجوم"
	askAP_option_1="(التوأم الشرير) FakeAP - Hostapd ("$red"انصحك بهادي)"
	askAP_option_2="(التوأم الشرير) FakeAP - airbase-ng (اتصال مسريعش)"
	askAP_option_3="التخمين على كلمة السر"
	# >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>
	header_askauth="طريقة التحقق من كلمة السر"
	askauth_option_1="Handshake ("$red"انصحك بهادي)"
	askauth_option_2="Wpa_supplicant(نسبة فشل مرتفعة)"
	# >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>
	header_deauthforce="Handshake بحث عن"
	deauthforce_option_1="aircrack-ng (نسبة فشل مرتفعة)"
	# >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>
